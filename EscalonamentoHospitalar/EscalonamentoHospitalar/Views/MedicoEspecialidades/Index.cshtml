@model IEnumerable<EscalonamentoHospitalar.Models.MedicoEspecialidade>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Medico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Especialidade)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Medico.MedicoId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Especialidade.EspecialidadeId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MedicoEspecialidadeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MedicoEspecialidadeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MedicoEspecialidadeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>

@model EscalonamentoHospitalar.Models.EspecialidadeEnfermeiro

@{
    ViewData["Title"] = "Eliminar";
}

<h2>Eliminar Especialidade de Enfermeiros</b></h2>

<h3>Tem a certeza de que pretende eliminar a especialidade <b>@Html.DisplayFor(model => model.Especialidade)</b>?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Especialidade
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Especialidade)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="EspecialidadeEnfermeiroId" />
        <input type="submit" value="Eliminar" class="btn btn-default" /> |
        <a asp-action="Index">Voltar à lista</a>
    </form>
</div>
